{
  "name": "mood-track",
  "version": "1.0.0",
  "description": "An app that analyzes a user's most recent Spotify plays and recommends a track based off the mood of those tracks.",
  "main": "./server/server.js",
  "engines": {
    "node": "12.20.0"
  },
  "type": "module",
  "scripts": {
    "start": "node ./server/server.js",
    "startserver": "nodemon --exec babel-node server/server.js",
    "startclient": "webpack serve --config ./webpack.config.js --mode development",
    "watch": "webpack --watch"
  },
  "author": "Andrew Loop <andrewmloop@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.0",
    "querystring": "^0.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.7",
    "nodemon": "^2.0.6"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
