  "main": "./src/app.js",
  "engines": {
    "node": "12.20.0"
  },
  "type": "module",
  "scripts": {
    "start": "node ./src/app.js",
    "devstart": "nodemon --exec babel-node src/app.js"
  },
  "author": "Andrew Loop <andrewmloop@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.21.1",
